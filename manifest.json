{
  "manifest_version": 2,

  "name": "Let's Schedule",
  "description":"This extension schedule your task to your google calender.",
  "version":"1.0",

  "browser_action":{
    "default_icon":"icon.png",
    "default_popup":"popup.html",
    "default_title":"Let's Schedule"
  },

  "permission":[
    "activeTab",
    "http://*/*","https://*/*"
  ]
}
